<!DOCTYPE html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title - @TrailerOnline.BLL.MultiTenancy.TenantBLL.DefaultTenantTitle</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <link rel="stylesheet" type="text/css" href="https://code.jquery.com/ui/1.10.4/themes/cupertino/jquery-ui.css" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/foundation/foundation.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/foundation/foundation.mvc.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Areas/Service/Content/site.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Areas/Service/Content/social.css")" />
    
    @RenderSection("Header", false)
    <script type="text/javascript" src="/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>

